<div>
	{{#each partials.abc.locations}}
	<div class="location">
		<h3>{{name}}</h3>
		<a href="{{forecastUrl}}">
			<img src="{{iconUrl}}" alt="{{weather}}">
			{{weather}}, {{temp}}
		</a>
	</div>
	{{/each}}
	<small>Source: <a href="http://www.bing.com">Weather Underground</a></small>
</div>